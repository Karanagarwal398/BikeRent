<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.karan.bikerent">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bike_image"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CheckoutActivity"
            android:label="@string/title_activity_checkout"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity android:name=".BookingActivity"
            android:theme="@style/AppTheme.NoActionBar"    />
        <activity android:name=".CarActivity" />
        <activity android:name=".Bajaj" />
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".activity_SignIn" />
        <activity android:name=".activity_Login">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>/>
        </activity>

        <activity android:name=".SignUpActivity" />
        <activity android:name=".ForgottenActivity" />
        <activity
            android:name=".HomeActivity"
            android:label="Select Bike"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".WebActivity" />
        <activity
            android:name=".ProductActivity"
            android:label="@string/title_activity_product"
            android:parentActivityName=".BikeCategoryActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".FullOrderActivity" />
        <activity android:name=".BikeCategoryActivity" />
    </application>

</manifest>